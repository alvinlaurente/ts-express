{
  "name": "typescript-express-sequelize",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "@alvinlaurente",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec 'ts-node' ./src/index.ts",
    "start:dev": "nodemon --exec node ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all --debug",
    "db:drop": "sequelize db:drop",
    "db:migrate:refresh": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all --debug"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.27",
    "@types/sequelize": "^4.28.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.27.1",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.2",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "uuid": "^8.3.2"
  },
  "_moduleAliases": {
    "@config/*": "dist/config/*",
    "@controllers/*": "dist/controllers/*",
    "@database/*": "dist/database/*",
    "@middlewares/*": "dist/middlewares/*",
    "@routes/*": "dist/routes/*",
    "@utils/*": "dist/utils/*",
    "@views/*": "dist/views/*"
  }
}
